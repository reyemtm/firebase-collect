-outline:focus {\r\n  background: white;\r\n  border-width: 2px;\r\n}\r\n\r\n.icon {\r\n  height: 1.1em;\r\n  width: 1.1em;\r\n}\r\n\r\n.loading-background {\r\n  background: #ffffff82;\r\n  width: 100%;\r\n  position: absolute;\r\n  top: 0;\r\n  left: 0;\r\n  bottom: 0;\r\n  z-index: 2;\r\n}\r\n\r\n.loading {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  -webkit-transform: translate(-50%, -50%);\r\n  -ms-transform: translate(-50%, -50%);\r\n  transform: translate(-50%, -50%);\r\n  z-index: 2;\r\n}\r\n/**SPECTRE CSS MODALS**/\r\n\r\n.modal .modal-container {\r\n  max-height: 96vh;\r\n}\r\n.modal.modal-lg .modal-overlay {\r\n  background: #ffffffb3;\r\n}\r\n\r\n.modal .modal-body {\r\n  padding: 0.8rem 0.8rem;\r\n}\r\n/*SPECTRE CSS TOASTS*/\r\n.toast {\r\n  position: absolute;\r\n  bottom: 0;\r\n  z-index: 2;\r\n  height: 0;\r\n  transition: height 0.2s ease-in-out;\r\n  border: none;\r\n  padding: 0;\r\n  border-radius: 0;\r\n  overflow: hidden;\r\n}\r\n\r\n.toast-active {\r\n  padding: .4rem;\r\n  height: 40px;\r\n}\r\n\r\n.toast.toast-popup {\r\n  background: #fff;\r\n  color: inherit;\r\n}\r\n\r\n.toast.toast-full {\r\n  height: calc(100vh);\r\n}\r\n\r\n.btn.btn-clear.btn-expand:before {\r\n  content: \"\\21a5\";\r\n  font-size: x-large;\r\n}\r\n\r\n/**SPECTRE CSS FORMS**/\r\n.form-input[readonly] {\r\n  color: gray;\r\n}\r\n.form-group:not(:last-child) {\r\n  margin-bottom: .1rem;\r\n}\r\n.form-label {\r\n  padding: .1rem 0 0.2rem;\r\n}\r\n.form-input {\r\n  padding: .1rem .2rem;\r\n}\r\n\r\n/*****************/\r\n\r\n/*MAPBOX GL TWEAKS*/\r\n\r\n#map {\r\n  position: absolute;\r\n  top: 0;\r\n  width: 100%;\r\n  bottom: 0;\r\n  background: whitesmoke;\r\n}\r\n\r\n#mapToolbar {\r\n  position: absolute;\r\n  top: 0;\r\n  height: auto;\r\n  /* margin: 10px; */\r\n  display: flex;\r\n  width: 80%;\r\n  /* background-color: #ffffffa6; */\r\n  z-index: 2;\r\n  justify-content: space-between;\r\n  padding: 10px;\r\n}\r\n\r\n#mapToolbar .mapboxgl-ctrl.mapboxgl-ctrl-group {\r\n  background: transparent;\r\n  box-shadow: none;\r\n}\r\n#mapToolbar .btn.btn-lg {\r\n  width: 36px;\r\n  height: 37px;\r\n  border: none;\r\n  color: #333;\r\n  background: #fff;\r\n  box-shadow: 0 0 0 1px rgba(0, 0, 0, .1);\r\n  border: none;\r\n  line-height: 23px;\r\n  /* line-height: 0rem; */\r\n  /* border-radius: 50%; */\r\n}\r\n\r\n.mapboxgl-ctrl-top-right {\r\n  /* top: 44px; */\r\n}\r\n.mapboxgl-ctrl-top-left {\r\n  top: 44px;\r\n}\r\n\r\n#mapToolbarBottom {\r\n  position: absolute;\r\n  bottom: 1rem;\r\n  width: 100%;\r\n  margin: 0 auto;\r\n  text-align: center;\r\n  z-index: 2;\r\n  /* display: flex; */\r\n  /* justify-content: flex-end; */\r\n  /* margin-right: 2rem; */\r\n  /* right: 1rem; */\r\n}\r\n#mapToolbarBottom .btn {\r\n  border-radius: 50%;\r\n  margin: 0 0.2rem;\r\n}\r\n\r\n\r\n/* #buttons {\r\n  margin: 5px 0;\r\n  position: absolute;\r\n  bottom: 0;\r\n  width: 100%;\r\n}\r\n\r\n#buttons .btn {\r\n  width: 31%;\r\n} */\r\n\r\n.mapboxgl-ctrl-group:not(:empty),\r\n.mapboxgl-ctrl-geocoder.mapboxgl-ctrl {\r\n  -moz-box-shadow: 0 0 1px rgba(0, 0, 0, .1);\r\n  -webkit-box-shadow: 0 0 1px rgba(0, 0, 0, .1);\r\n  box-shadow: 0 0 0 1px rgba(0, 0, 0, .1);\r\n  border-radius: 2px;\r\n  /*       background: transparent; */\r\n}\r\n\r\n.mapboxgl-ctrl-group>button {\r\n  width: 36px;\r\n  height: 37px;\r\n}\r\n\r\n.mapboxgl-user-location-dot {\r\n  background-color:rgba(255,0,0,0.6);\r\n  width: 14px;\r\n  height: 14px;\r\n}\r\n\r\n.mapboxgl-user-location-dot::before {\r\n  background-color: transparent;\r\n  content: '';\r\n  width: 20px;\r\n  height: 20px;\r\n  border-radius: 0;\r\n  border-left: 2px solid #333;\r\n  left: 6px;\r\n  position: absolute;\r\n  animation: none;\r\n  top: -3px;\r\n}\r\n.mapboxgl-user-location-dot::after {\r\n  border-radius: 0;\r\n  border: none;\r\n  border-top: 2px solid #333;\r\n  content: '';\r\n  height: 19px;\r\n  left: -3px;\r\n  position: absolute;\r\n  top: 6px;\r\n  width: 20px;\r\n  box-sizing: border-box;\r\n  box-shadow: none;\r\n}\r\n\r\n.marker {\r\n  background-color: #fff;\r\n  width: 15px;\r\n  height: 15px;\r\n  border-radius: 50%;\r\n  box-shadow: 0 0 2px rgba(0, 0, 0, 0.25);\r\n}\r\n\r\n/********************/\r\n"},"lineCount":262}},"error":null,"hash":"36f8484600e460a93884a0b76e911e0a","cacheData":{"env":{}}}